<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="web.dao.face.CartDao">
	<select id="selectCartByUserno" resultType="web.dto.Cart" parameterType="web.dto.Cart">
		select C.*, S.* from cart C, source S
		where C.source_no = S.source_no
		and C.user_no = #{userNo};
	
	</select>


</mapper>  